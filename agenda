          <div class="card">
            <div class="card-body">
              <div>
                <h5>Sumário Executivo</h5>
                <!-- Renderiza como innerHTML -->
                <div [innerHTML]="item.sumario_executivo_rel"></div>
              </div>
              <div class="mt-3">
                <h5>Racional do Sumário</h5>
                <!-- Racional como texto simples -->
                <p>{{ item.racional_sumario_rel }}</p>
              </div>
            </div>
          </div>
          <hr>
        </div>
      </div>
    </div>

